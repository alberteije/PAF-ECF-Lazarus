<?xml version="1.0" encoding="utf-8"?>
<t2ti-orm-mapping>
  <class name="ContadorVO" table="contador" >

    <id name="Id" column="ID">
      <generator class="sequence">
        <param name="sequence">contador</param>
      </generator>
    </id>

    <property name="Nome" column="NOME" caption="Nome" width="450" type="String" />
    <property name="Cpf" column="CPF" caption="Cpf" width="88" type="String" />
    <property name="Cnpj" column="CNPJ" caption="Cnpj" width="112" type="String" />
    <property name="InscricaoCrc" column="INSCRICAO_CRC" caption="Inscricao Crc" width="120" type="String" />
    <property name="UfCrc" column="UF_CRC" caption="Uf Crc" width="16" type="String" />
    <property name="Fone" column="FONE" caption="Fone" width="112" type="String" />
