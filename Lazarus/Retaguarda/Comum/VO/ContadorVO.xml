    <property name="Logradouro" column="LOGRADOURO" caption="Logradouro" width="450" type="String" />
    <property name="Numero" column="NUMERO" caption="Numero" width="80" type="String" />
    <property name="Complemento" column="COMPLEMENTO" caption="Complemento" width="450" type="String" />
    <property name="Bairro" column="BAIRRO" caption="Bairro" width="450" type="String" />
    <property name="Cidade" column="CIDADE" caption="Cidade" width="450" type="String" />
    <property name="Cep" column="CEP" caption="Cep" width="64" type="String" />
    <property name="MunicipioIbge" column="MUNICIPIO_IBGE" caption="Municipio Ibge" width="80" type="Integer" />
    <property name="Uf" column="UF" caption="Uf" width="16" type="String" />
    <property name="Email" column="EMAIL" caption="Email" width="450" type="String" />
    <property name="Site" column="SITE" caption="Site" width="450" type="String" />

  </class>
</t2ti-orm-mapping>
